	%attrs;
    name	CDATA	#IMPLIED
	height	CDATA	#IMPLIED
	width	CDATA	#IMPLIED
	param	CDATA	#IMPLIED
    src		%URL	#REQUIRED  -- URL of image to embed --
		>

<!ELEMENT NOEMBED	- -	EMPTY>

]]>


